  "scripts": {
    "preserve": "npm run build && npm run sass",
    "serve": "nodemon --watch public && npm run sass-watch",
    "build": "node server/build.js",
    "sass": "node-sass -o public/assets/css/ --output-style compressed --source-map public/assets/css/ scss",
    "presass-watch": "npm run sass",
    "sass-watch": "node-sass -w -o public/assets/css/ --output-style compressed --source-map public/assets/css/ scss"
  },
  "dependencies": {
    "express": "^4.13.3",
    "fs-extra": "^0.26.2",
    "useragent": "^2.1.8"
  },
